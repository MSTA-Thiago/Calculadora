
{
    
    "version": 2,
    "builds": [
        {
            "src": "api/flask_app.py",
            "use": "@vercel/python",
            "config": {"maxDuration": 200}
           }
            ] ,

    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/flask_app.py"
        }
            ],

    "functions": {
        "api/**/*": {
            "maxDuration": 5 
        }
        }        


}

