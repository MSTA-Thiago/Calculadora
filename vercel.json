{
    "builds": [
        {
            "src": "flask_app.py",
            "use": "@vercel/python",
            "config": {"maxDuration": 200}
           }
            ] ,

    "routes": [
        {
            "src": "/(.*)",
            "dest": "flask_app.py"
        }
            ],
  
    "functions": {
    "/*.py": {
      "maxDuration": 200}}
}
